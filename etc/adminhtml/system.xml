<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../Magento/Config/etc/system_file.xsd">
    <system>
        <tab id="magehqm2" translate="label" sortOrder="300">
            <label>Magehqm2 Extensions</label>
        </tab>
       <section id="magehqm2license" translate="label" type="text" sortOrder="0" showInDefault="1" showInWebsite="1" showInStore="1">
            <class>separator-top</class>
            <label>Manage Licenses</label>
            <tab>magehqm2</tab>
            <resource>Magehqm2_Core::config</resource>
            <group id="general" translate="label" type="text" sortOrder="0" showInDefault="1" showInWebsite="1" showInStore="1">
                <field id="licenses" translate="label comment" type="text" sortOrder="30" showInDefault="1" showInWebsite="1" showInStore="1">
                    <label>Manage Licenses</label>
                    <frontend_model>Magehqm2\Core\Block\Adminhtml\System\ListLicense</frontend_model>
                </field>
            </group>
        </section>
    </system>
</config>